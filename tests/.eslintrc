{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "consistent-return": 0,
    "no-unused-expressions": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "expect": true,
    "sinon": true,
    "MAIN": true
  }
}
